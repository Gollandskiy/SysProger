<Window x:Class="Занятие_в_аудитории_1_Системное_программирование_.DLLWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Занятие_в_аудитории_1_Системное_программирование_"
        mc:Ignorable="d"
        Title="DLLWindow" Height="450" Width="300">
    <StackPanel>
        <Button x:Name="AlertButton" Content="Alert"
                Click="AlertButton_Click" Margin="10 5"/>
        <Button x:Name="ThreadButton" Content="Thread"
                Click="ThreadButton_Click" Margin="10 5"/>

        <StackPanel Orientation="Horizontal">
            <Button x:Name="SoundButton" Content="Sound 1"
                Click="SoundButton_Click" Margin="10 5"/>
            <Button x:Name="SoundButton2" Content="Sound 2"
                Click="SoundButton2_Click" Margin="10 5"/>
            <Button x:Name="SoundButton3" Content="Sound 3"
                Click="SoundButton3_Click" Margin="10 5"/>
            <Button x:Name="SoundButton4" Content="Sound 4"
                Click="SoundButton4_Click" Margin="10 5"/>
        </StackPanel>
    </StackPanel>
</Window>
